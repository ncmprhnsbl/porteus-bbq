# This is an example for IceWM's menu definition file.
#
# Place your variants in /etc/icewm or in $HOME/.icewm
# since modifications to this file will be discarded when you
# (re)install icewm.
#
prog Terminal /usr/share/icons/Vibrancy-Colors/apps/16/terminal.png lxterminal
prog Filemanger /usr/share/icons/Vibrancy-Colors/apps/16/system-file-manager.png lxterminal -e mc --skin=nicedark.ini
prog Editor /usr/share/icons/Vibrancy-Colors/apps/16/accessories-text-editor.png lxterminal -e mcedit --skin=nicedark.ini
prog WebBrowser /usr/share/icons/Vibrancy-Colors/apps/16/webbrowser-app /opt/porteus-scripts/xorg/browser
separator 
menufile Programs folder programs
menufile Tool_bar folder toolbar
